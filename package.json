{
  "name": "pnpm-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*"
    ]
  },
  "scripts": {
    "start:ui": "pnpm --filter ui dev",
    "start:api": "pnpm --filter api start:dev",
    "start": "pnpm run start:ui & pnpm run start:api",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs --cache --fix",
    "build": "pnpm recursive run build",
    "build-all": "nx run-many --target=build --all",
    "test": "pnpm recursive run test",
    "test-all": "nx run-many --target=test --all",
    "test:e2e": "nx run-many --target=e2e --all",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [],
  "author": "The Cometbid Technology Foundation Inc.",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "@pnpmworkspace/logger": "link:packages/logger"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@nx/cypress": "20.7.1",
    "@nx/eslint": "20.7.1",
    "@nx/eslint-plugin": "20.7.1",
    "@nx/jest": "20.7.1",
    "@nx/js": "20.7.1",
    "@nx/next": "20.7.1",
    "@nx/react": "20.7.1",
    "@nx/vite": "20.7.1",
    "@nx/web": "20.7.1",
    "@swc-node/register": "~1.10.10",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.16",
    "@swc/helpers": "~0.5.15",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitest/ui": "^3.1.1",
    "babel-jest": "^29.7.0",
    "cypress": "^14.2.1",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jiti": "2.4.2",
    "nx": "20.7.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "ts-jest": "^29.3.1",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "8.29.0",
    "vite": "^6.2.7",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "nx"
    ]
  }
}
